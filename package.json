{
  "name": "chARpMolecularBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "_build": "npm run clean && svelte-kit build",
    "clean": "trash build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "_deploy": "node ./gh-pages.js",
	"__deploy": "touch build/.nojekyll && gh-pages -d build -t true",
	"deploy": "rm -rf build && npm run build && touch build/.nojekyll && npx gh-pages -d build -t true"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@sveltejs/adapter-auto": "^1.0.0-next.90",
    "@sveltejs/adapter-static": "^1.0.0-next.50",
    "@sveltejs/kit": "^1.0.0-next.587",
    "gh-pages": "^5.0.0",
    "mdsvex": "^0.10.6",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "trash-cli": "^5.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module"
}
